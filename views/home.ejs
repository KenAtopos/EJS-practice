<%- include('partials/header'); -%>

    <h1>Home</h1>
    <p>
        <%= homeSC %>
    </p>

    <% posts.forEach(element => { %>
        <div>
            <h1>
                <%= element.title %>
            </h1>
            <p class="truncate">
                <%= element.body %>
            </p>
        </div>
        <a href="/blogs/<%=element._id %>">Read more</a>
    <% }); %>
        

<%- include('partials/footer'); -%>